{
  "name": "Ampersand",
  "description": "Ampersand JS Quickstart Project",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run sass\" ",
    "build": "sass src/stylesheets/main.scss src/www/style.css --style compressed; webpack -p --config webpack-production.config.js --progress --colors",
    "dev": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "sass": "sass --watch src/stylesheets/main.scss:src/www/style.css --style compressed"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "lodash-webpack-plugin": "^0.10.5",
    "node-sass": "^3.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "underscore-template-loader": "^0.7.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-material-design-icons": "0.0.2"
  },
  "dependencies": {
    "ampersand-collection": "^2.0.0",
    "ampersand-events": "^2.0.2",
    "ampersand-model": "^8.0.0",
    "ampersand-rest-collection": "^6.0.0",
    "ampersand-router": "^4.0.0",
    "ampersand-view": "^10.0.1",
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "chart.js": "^2.4.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.1",
    "material-datepicker": "0.0.1",
    "material-datetime-picker": "^1.0.1",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.2.1",
    "materialize-css": "^0.97.8"
  }
}
